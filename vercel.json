{
    "builds": [{
        "src": "carzone/wsgi.py",
        "use": "@vercel/python",
        "config": { 
            "maxLambdaSize": "15mb", 
            "runtime": "python3.9" 
        }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "carzone/wsgi.py"
        }
    ],
    "installCommand": "pip install psycopg2-binary && pip install --disable-pip-version-check --target . --upgrade -r requirements.txt"
}
