{
    "builds": [{
        "src": "carzone/wsgi.py",
        "use": "@vercel/python",
        "config": {
            "maxLambdaSize": "15mb",
            "runtime": "python3.9"
        }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "carzone/wsgi.py"
        }
    ],
    "installCommand": "pip install --disable-pip-version-check --target . --upgrade -r requirements.txt || true",
    "hooks": {
        "postinstall": "pip uninstall psycopg2 -y && pip install psycopg2-binary"
    }
}
